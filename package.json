    "@types/react-native-vector-icons": "6.4.12",
    "@types/semver": "7.3.13",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "appcenter": "4.4.5",
    "appcenter-analytics": "4.4.5",
    "appcenter-crashes": "4.4.5",
    "axios": "1.2.1",
    "babel-plugin-module-resolver": "4.1.0",
