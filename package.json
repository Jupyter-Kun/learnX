    "@react-navigation/native": "6.1.0",
    "@react-navigation/native-stack": "6.9.5",
    "@reduxjs/toolkit": "1.9.1",
    "@tsconfig/react-native": "2.0.3",
    "@types/jest": "29.2.4",
    "@types/mime-types": "2.1.1",
    "@types/react": "18.0.26",
